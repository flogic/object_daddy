ActiveRecord::Schema.define(:version => 0) do
  create_table :frobnitzs, :force => true do |t|
    t.column :name, :string
    t.column :title, :string
    t.column :foo_id, :integer
    t.column :baz_id, :integer
    t.column :thing_id, :integer
    t.column :splort_id, :integer
    t.column :bangbang_id, :integer
    t.column :wakka_id, :integer
    t.column :blitblot_id, :integer
    t.column :poolie_id, :integer
  end

  create_table :foos, :force => true do |t|
    t.column :name, :string
  end

  create_table :bazs, :force => true do |t|
    t.column :name, :string
  end

  create_table :splorts, :force => true do |t|
    t.column :name, :string
  end

  create_table :boogs, :force => true do |t|
    t.column :bam, :string
  end

  create_table :bars, :force => true do |t|
    t.column :name, :string
  end

  create_table :things, :force => true do |t|
    t.column :name, :string
  end

  create_table :blahs, :force => true do |t|
    t.column :bam, :string
  end

  create_table :ya_models, :force => true do |t|
    t.column :name, :string
  end

  create_table :corks, :force => true do |t|
    t.column :name, :string
  end

  create_table :protected_attributes, :force => true do |t|
    t.column :public_name, :string
    t.column :private_name, :string
  end
end
